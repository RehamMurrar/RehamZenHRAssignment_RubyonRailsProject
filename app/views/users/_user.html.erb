<div id="<%= dom_id user %>">
  <p>
    <strong>User:</strong>
    <%= user.user_id %>
  </p>

  <p>
    <strong>User name:</strong>
    <%= user.user_name %>
  </p>

  <p>
    <strong>User type:</strong>
    <%= user.user_type.user_type_name %>
  </p>

  <p>
    <strong>User email:</strong>
    <%= user.user_email %>
  </p>

  <p>
    <strong>User password:</strong>
    <%= user.user_password %>
  </p>

</div>
